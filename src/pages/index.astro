---
import BaseLayout from '../layouts/BaseLayout.astro';
import '../assets/styles/new_guide.scss';

// 페이지별 데이터: 예시로 navigation 데이터를 전달합니다.
const navigation = [
    {name: 'Main', url: '../index.html'},
    {name: 'Docs', url: '../docs.html'},
    {
        name: 'Components',
        children: [
            {name: 'Tab', url: '../components/Tab.html'},
            {name: 'Accordion', url: '../components/Accordion.html'},
            {name: 'Target Button', url: '../components/TargetButton.html'},
            {name: 'Copy Button', url: '../components/CopyButton.html'},
            {name: 'Scroll Button', url: '../components/ScrollButton.html'}
        ]
    },
    {name: 'Form', url: '../form.html'},
    {name: 'Download', url: '../download.html'},
    {name: 'Admin', url: '../admin/admin.html'}
];
---

<BaseLayout pageTitle="FP 프로젝트 - Main" navigation={navigation}>

</BaseLayout>

<script src="../assets/js/custom/pf.js"></script>