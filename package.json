{
  "name": "publishing-factory",
  "version": "1.0.0",
  "description": "```sh npm create astro@latest -- --template basics ```",
  "main": "index.js",
  "scripts": {
    "eleventy": "eleventy --config .eleventy.js",
    "eleventy:watch": "eleventy --config .eleventy.js --watch",
    "dev": "concurrently \"npm run eleventy:watch\" \"vite\"",
    "build": "cross-env PROJECT_NAME=wv npm run _build",
    "_build": "npm run eleventy && vite build",
    "docs": "cross-env PROJECT_NAME=wv npm run eleventy && vite build --mode docs",
    "preview": "vite preview --port 3000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "sass": "^1.85.1",
    "vite": "^6.2.0"
  }
}
